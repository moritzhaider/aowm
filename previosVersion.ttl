@prefix aowm: <http://dqm.faw.jku.at/ontologies/aowm#> .             
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .        
@prefix vann: <http://purl.org/vocab/vann/> .      
@prefix foaf: <http://xmlns.com/foaf/0.1/> .       
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix mod: <https://w3id.org/mod#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix schema: <http://schema.org/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .

#################################################################
# Ontology Header 
#################################################################
<http://dqm.faw.jku.at/ontologies/aowm>
    a owl:Ontology ;
    dct:title "Vocabulary For Annotating Ontologies With Metadata"@en ;
    dct:creator "Moritz Haider" ;
    dct:description "An ontology designed to describe metadata about other ontologies, such as versioning, creation date, author, and documentation links."@en ;
    dct:created "2025-11-11"^^xsd:date ;
    dct:license <http://creativecommons.org/licenses/by/4.0/> ;
    vann:preferredNamespacePrefix "aowm" ;
    vann:preferredNamespaceUri <http://dqm.faw.jku.at/ontologies/aowm#> ;
    owl:versionInfo "0.1" ;
    .

#################################################################
# Classes
#################################################################
aowm:Resource
    a owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:comment "Generic parent element for all domain classes in the AOWM vocabulary. Used as the common superclass for ontology metadata resources (Elements, Version, ...)."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf owl:Thing .

aowm:Person
    a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "Describes a person who is related in some way to an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Resource .

aowm:Author
    a owl:Class ;
    rdfs:label "Author"@en ;
    rdfs:comment "A person responsible for authoring an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Person .

aowm:Contributor
    a owl:Class ;
    rdfs:label "Contributor"@en ;
    rdfs:comment "A person responsible for contributing to an ontology in some way."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Person .

aowm:Publisher
    a owl:Class ;
    rdfs:label "Publisher"@en ;
    rdfs:comment "A person responsible for publishing an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Person .

aowm:Version
    a owl:Class ;
    rdfs:label "Version"@en ;
    rdfs:comment "The version of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Resource .

aowm:Ontology
    a owl:Class ;
    rdfs:label "Ontology"@en ;
    rdfs:comment "A formal specification of a conceptualization, described using RDF-based standards."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Resource .

aowm:Changes
    a owl:Class ;
    rdfs:label "Changes to Predecessor Version"@en ;
    rdfs:comment "Capture changes of classes/properties of an ontology compared to a predecessor version."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Resource .

aowm:Element
    a owl:Class ;
    rdfs:label "Elements of an ontology"@en ;
    rdfs:comment "Include all elements of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Ontology .

aowm:Class
    a owl:Class ;
    rdfs:label "Classes of an ontology"@en ;
    rdfs:comment "Include all classes of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Element .

aowm:DataProperty
    a owl:Class ;
    rdfs:label "Data properties of an ontology"@en ;
    rdfs:comment "Include all data properties of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Element .

aowm:ObjectProperty
    a owl:Class ;
    rdfs:label "Object properties of an ontology"@en ;
    rdfs:comment "Include all object properties of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Element .

aowm:Constraint
    a owl:Class ;
    rdfs:label "Constraints of an ontology"@en ;
    rdfs:comment "Include all constraints of an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Element .

aowm:TypicalMetadata
    a owl:Class ;
    rdfs:label "Typical classic metadata"@en ;
    rdfs:comment "Class for capturing typical metadata of an ontology like a title, description, purpose and so on."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    rdfs:subClassOf aowm:Resource .

#################################################################
# Object Properties
#################################################################

aowm:hasVersion
    a owl:ObjectProperty ;
    rdfs:label "has version"@en ;
    rdfs:domain aowm:Ontology ;
    rdfs:range aowm:Version ;
    rdfs:comment "Specifies the version an ontology has."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:predecessorVersion
    a owl:ObjectProperty ;
    rdfs:label "has predecessor version"@en ;
    rdfs:domain aowm:Version ;
    rdfs:range aowm:Version ;
    rdfs:comment "Specifies the predecessor version an ontology version has."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:madeChange
    a owl:ObjectProperty ;
    rdfs:label "who is responsible for the change"@en ;
    rdfs:domain aowm:Version ;
    rdfs:range aowm:Person ;
    rdfs:comment "Specifies the person which changed something in this version."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:hasAuthor
    a owl:ObjectProperty ;
    rdfs:label "is author of"@en ;
    rdfs:domain aowm:Ontology ;
    rdfs:range aowm:Author ;
    rdfs:comment "Links an author to the ontology they created or contributed to."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:documentation
    a owl:ObjectProperty ;
    rdfs:domain aowm:Ontology ;
    rdfs:range foaf:Document ;
    rdfs:label "documentation"@en ;
    rdfs:comment "Links an ontology to its documentation resource."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ; .

aowm:creationDate
    a owl:DatatypeProperty ;
    rdfs:domain aowm:Version ;
    rdfs:range xsd:date ;
    rdfs:label "creation date"@en ;
    rdfs:comment "Specifies the creation date of a specific ontology version."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ; .
	
aowm:changedResource 
	a owl:ObjectProperty ;
    rdfs:domain aowm:Version ;
    rdfs:range aowm:Resource ;
    rdfs:label "changed resource"@en ;
    rdfs:comment "Specifies the resources that were changed in this version compared to the predecessor version."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
#wer hat change gemacht
aowm:madeChange 
	a owl:ObjectProperty ;
    rdfs:domain aowm:Version ;
    rdfs:range aowm:Person ;
    rdfs:label "who is responsible for the change"@en ;
    rdfs:comment "Specifies the person which changed something in this version."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:hasDatatypeProperty
	a owl:ObjectProperty ;
	rdfs:domain aowm:Element ;
	rdfs:range aowm:DatatypeProperty ;
	rdfs:label "Ontology has element datatype property"@en ;
    rdfs:comment "Specifies an datatype property."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:hasObjectProperty
	a owl:ObjectProperty ;
	rdfs:domain aowm:Element ;
	rdfs:range aowm:ObjectProperty ;
	rdfs:label "Ontology has element object property"@en ;
    rdfs:comment "Specifies an object property."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:hasClass
	a owl:ObjectProperty ;
	rdfs:domain aowm:Element ;
	rdfs:range aowm:Class ;
	rdfs:label "Ontology has class"@en ;
    rdfs:comment "Specifies a class."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:hasConstraint
	a owl:ObjectProperty ;
	rdfs:domain aowm:Element ;
	rdfs:range aowm:Constraint ;
	rdfs:label "Ontology has constraint"@en ;
    rdfs:comment "Specifies a constraint."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:versionIRI
	a owl:ObjectProperty ;
    rdfs:subPropertyOf owl:versionIRI ;
    rdfs:domain aowm:Ontology ;
    rdfs:range aowm:Ontology ;
    rdfs:label "Version IRI"@en ;
    rdfs:comment "IRI which identifies the version of the ontology."@en ;
	rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:backwardCompatibleWith
	a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:backwardCompatibleWith ;
	rdfs:domain aowm:Ontology ;
	rdfs:range aowm:Ontology ;
	rdfs:label "Backward Compatibility"@en ;
    rdfs:comment "Is this ontology version compatible with previous ones."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:incompatibleWith
	a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:incompatibleWith ;
	rdfs:domain aowm:Ontology ;
	rdfs:range aowm:Ontology ;
	rdfs:label "Backward Compatibility"@en ;
    rdfs:comment "Is this ontology version compatible with previous ones."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:fundedBy
	a owl:ObjectProperty ;
	rdfs:subPropertyOf schema:funder ;
	rdfs:domain aowm:Ontology ;
	rdfs:range schema:Organization ;
	rdfs:label "Funder"@en ;
    rdfs:comment "Funded by"@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:license
	a owl:ObjectProperty ;
	rdfs:subPropertyOf dct:license ;
	rdfs:domain aowm:Ontology ;
	rdfs:range dct:LicenseDocument ;
	rdfs:label "License"@en ;
	rdfs:comment "License."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:visualization
	a owl:ObjectProperty ;
	rdfs:subPropertyOf schema:image ;
	rdfs:domain aowm:Ontology ;
	rdfs:range schema:ImageObject ;
	rdfs:label "Visualization"@en ;
	rdfs:comment "Visu of ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:logo
	a owl:ObjectProperty ;
	rdfs:subPropertyOf schema:image ;
	rdfs:domain aowm:Ontology ;
	rdfs:range schema:ImageObject ;
	rdfs:label "Logo"@en ;
	rdfs:comment "Logo of ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
#################################################################
# Datatype Properties
#################################################################
aowm:hasName
    a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:domain aowm:Person ;
    rdfs:range xsd:string ;
    rdfs:comment "Full name of the person."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:hasEmail
    a owl:DatatypeProperty ;
    rdfs:label "has email"@en ;
    rdfs:domain aowm:Person ;
    rdfs:range xsd:string ;
    rdfs:comment "Email address of the person."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
  
aowm:hasAffiliation
    a owl:DatatypeProperty ;
    rdfs:label "affiliation"@en ;
    rdfs:domain aowm:Person ;
    rdfs:range foaf:Organization ;
    rdfs:comment "Organization the person is affiliated with."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    .
    
aowm:hasRole
    a owl:DatatypeProperty ;
    rdfs:label "has role"@en ;
    rdfs:domain aowm:Author ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies the author's role, such as main author, editor or for which part the author is responsible."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> ;
    .
    
 aowm:hasContributed
	a owl:DatatypeProperty ;
    rdfs:label "has contributed"@en ;
    rdfs:domain aowm:Contributor ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies the contributor's role in which manner he contributed to an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> 
   	.

aowm:hasPublished
	a owl:DatatypeProperty ;
    rdfs:label "has published"@en ;
    rdfs:domain aowm:Publisher ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies who published an ontology."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> 
    .
    
aowm:hasChangesToPredecessorVersion
    a owl:DatatypeProperty ;
    rdfs:label "changes to predecessor version"@en ;
    rdfs:domain aowm:Version ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies the changes of an ontology version in contrast to a predecessor version, always mention what has changed (full URI)."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
    

aowm:preferredNamespaceUri 
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf vann:preferredNamespaceUri ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:anyURI ;
    rdfs:label "Preferred namespace URI"@en ;
    rdfs:comment "Preferred namespace URI for the ontology."@en .

aowm:preferredNamespacePrefix
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf vann:preferredNamespacePrefix ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Preferred namespace prefix"@en ;
    rdfs:comment "Preferred namespace URI for the ontology."@en .

aowm:hasVersionName
    a owl:DatatypeProperty ;
    rdfs:label "has version name"@en ;
    rdfs:domain aowm:Version ;
    rdfs:range xsd:string ;
    rdfs:comment "Specifies the version name."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:hasTitle
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf dct:title ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Title of the ontology"@en ;
    rdfs:comment "Title."@en .

aowm:hasDescription
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf dct:description ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Description of the ontology"@en ;
    rdfs:comment "Detailed description."@en .

aowm:hasAbstract
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf dct:abstract ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Abstract of the ontology"@en ;
    rdfs:comment "Abstract."@en .
	
aowm:status
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf mod:status ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Current Status of the ontology"@en ;
    rdfs:comment "Status."@en .
	
aowm:usageNote
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf vann:usageNote ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:string ;
    rdfs:label "Usage of the ontology"@en ;
    rdfs:comment "Usage notes."@en .

aowm:created
    a owl:DatatypeProperty ;
    rdfs:subPropertyOf dct:created ;
    rdfs:domain aowm:Ontology ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Creation Date"@en ;
    rdfs:comment "Creation Date."@en ;
	rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
    
aowm:source
	a owl:DatatypeProperty ;
    rdfs:subPropertyOf dct:source ;
    rdfs:domain aowm:Ontology ;
    rdfs:range rdfs:Resource ;
    rdfs:label "Creation Date"@en ;
    rdfs:comment "Creation Date."@en ;
	rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:extends
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf voaf:extens ;
	rdfs:domain aowm:Ontology ;
	rdfs:range rdfs:Resource ;
	rdfs:label "Extends"@en ;
	rdfs:comment "Extentds"@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .
	
aowm:fundedByName
	a owl:DatatypeProperty ;
	rdfs:domain aowm:Ontology ;
	rdfs:range xsd:string ;
	rdfs:label "Funded By"@en ;
	rdfs:comment "Specifies the name of the funding organization as a literal value."@en ;
    rdfs:isDefinedBy <http://dqm.faw.jku.at/ontologies/aowm> .

aowm:doi
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf bibo:doi ;
	rdfs:domain aowm:Ontology ;
	rdfs:range xsd:string ;
	rdfs:label "DOI"@en ;
